{
  "name": "eslint-config-terra",
  "version": "2.1.0",
  "description": "Terra Eslint Config",
  "main": "./packages/eslint-config-terra/eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/eslint-config-terra"
  },
  "keywords": [
    "eslint-config",
    "Cerner",
    "Terra"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/eslint-config-terra/issues"
  },
  "homepage": "https://github.com/cerner/eslint-config-terra",
  "scripts": {
    "bootstrap:hoist": "lerna bootstrap --hoist",
    "clean:all": "lerna clean --yes && shx rm -rf node_modules",
    "clean:install": "npm run clean:all && npm install",
    "danger": "lerna run danger ci",
    "publish": "npm whoami && npm test && lerna publish",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "lerna run test:lint",
    "test:unit": "lerna run test:unit"
  },
  "devDependencies": {
    "danger": "^3.7.14",
    "eslint": "^5.0.0",
    "lerna": "^3.8.0",
    "rimraf": "^2.6.1",
    "shx": "^0.3.2"
  }
}
